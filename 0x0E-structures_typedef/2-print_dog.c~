void print_dog(struct dog *d)
{
	if (d == NULL)
	{
		printf(" ");
		exit(EXIT_FAILURE);
	}
	if (d->name == NULL)
		d->name = "nil";\
	if (d->age == NULL)
		d->age = "nil";
	if (d->owner == NULL)
		d->owner = "nil";
	printf("Name: %s\n Age: %f\n Owner: %s\n", 
}
